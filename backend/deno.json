{
  "imports": {
    "@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@^3.830.0",
    "@hono/zod-validator": "npm:@hono/zod-validator@^0.5.0",
    "argon2": "npm:argon2@^0.43.0",
    "@/": "./",
    "dotenv": "npm:dotenv@^16.5.0",
    "drizzle-kit": "npm:drizzle-kit@^0.31.1",
    "drizzle-orm": "npm:drizzle-orm@^0.44.2",
    "hono": "npm:hono@^4.8.0",
    "ioredis": "npm:ioredis@^5.6.1",
    "nodemailer": "npm:nodemailer@^7.0.3",
    "pg": "npm:pg@^8.16.1",
    "pusher": "npm:pusher@^5.2.0",
    "zod": "npm:zod@^3.25.67"
  },
  "tasks": {
    "start": "deno run --allow-net main.ts",
    "dev": "deno run --allow-net --watch main.ts"
  },
  "compilerOptions": {
